{
  "name": "Location reference app CSV data file",
  "code": "pilrhealth:location_ref_app:location_csv",
  "version": 1,
  "description": "A CSV data file output from the location reference application",
  "definition_type": "filetype",
  "dataset": "pilrhealth:location_ref_app:location",
  "dataset_version": 1,
  "parser": {
    "parse_by": "column_index",
    "col_name_row": 1,
    "skip_rows": 1,
    "date_format": "yyyy-MM-dd hh:mm:ss aa",
    "data_mappings": [
      {
        "from_index": 3,
        "to_name": "lat"
      },
      {
        "from_index": 4,
        "to_name": "lon"
      },
      {
        "from_index": 5,
        "to_name": "provider"
      },
      {
        "from_index": 6,
        "to_name": "speed"
      },
      {
        "from_index": 7,
        "to_name": "time"
      },
      {
        "from_index": 8,
        "to_name": "alt"
      },
      {
        "from_index": 9,
        "to_name": "acc"
      },
      {
        "from_index": 10,
        "to_name": "bearing"
      }
    ],
    "metadata_mappings": [
      {
        "from_index": 0,
        "to_name": "id"
      },
      {
        "from_index": 1,
        "to_name": "pt"
      },
      {
        "from_index": 2,
        "to_name": "timestamp"
      }
    ]
  }
}
